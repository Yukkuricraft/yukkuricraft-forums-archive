{
  "name": "yukkuricraft-forums-archive",
  "version": "1.0.0",
  "workspaces": [
    "frontend",
    "backend",
    "packages/*"
  ],
  "scripts": {
    "lint": "turbo lint",
    "fixlint": "turbo fixlint",
    "format-check": "prettier --check \"**/*.(md|yaml|json|js|ts|vue)\"",
    "format": "prettier --write \"**/*.(md|yaml|json|js|ts|vue)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukkuricraft/yukkuricraft-forums-archive.git"
  },
  "private": true,
  "packageManager": "yarn@4.9.1",
  "devDependencies": {
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vue-eslint-parser": "^10.1.3"
  }
}
